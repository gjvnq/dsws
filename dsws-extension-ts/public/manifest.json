{
   "name": "DSWS Extension",
   "description": "This extension is made for demonstration purposes",
   "icons": {
      "48":"favicon.ico"
   },
   "version": "0.0.1",
   "manifest_version": 3,
   "permissions": [
      "history"
   ],
   "host_permissions": [
      "*://dsws.localhost/*"
   ],
   "background": {
      "service_worker": "dsws_worker.js"
   },
   "action": {
      "default_icon": "favicon.ico",
      "default_title": "DSWS"
   }
}